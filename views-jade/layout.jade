!!!
html
  head
    block head
      title= 'Agenda Book'
      link(rel='stylesheet', href='/bootstrap/css/bootstrap.min.css', type='text/css')
      link(rel='stylesheet', href='/css/style.css', type='text/css')
      script(src='/js/vendor/lodash.min.js', type='text/javascript')
      script(src='/js/vendor/jquery-1.10.2.min.js', type='text/javascript')
      script(src='/js/vendor/jquery.actual.js', type='text/javascript')
      script(src='/js/vendor/angular.min.js', type='text/javascript')
      script(src='/js/vendor/angular-route.min.js', type='text/javascript')
      script(src='/js/vendor/angular-resource.min.js', type='text/javascript')
      script(src='/bootstrap/js/bootstrap.min.js', type='text/javascript')
      script(src='/js/main.js', type='text/javascript')
      script(type='text/javascript').
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-41411244-1', 'mbilker.us');
        ga('send', 'pageview');
  body
    .container#app
      div#header
        h1 Agenda Book
        - if (settings.info && settings.info.length != 0)
          div
            each message in settings.info
              span.label.label-info= message
        - if (settings.error && settings.error.length != 0)
          div
            each message in settings.error
              span.label.label-danger= message
        - if (typeof currentUser !== 'undefined')
          a#logout(href='/sessions') Log Out
      block content
    block footer
